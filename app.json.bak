{
  "expo": {
    "name": "Miles Client",
    "slug": "miles-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danish.milesclient.dev",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "location"]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.${ios.bundleIdentifier}.onesignal"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "ACCESS_BACKGROUND_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "WRITE_EXTERNAL_STORAGE",
        "WAKE_LOCK",
        "VIBRATE"
      ],
      "package": "com.mujeorg.milesclient",
      "versionCode": 1
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      "expo-secure-store",
      "expo-task-manager",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isAndroidBackgroundLocationEnabled": true,
          "isIosBackgroundLocationEnabled": true
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "75bb3ac1-a958-44c2-989e-6084b19f60ee"
      },
      "oneSignalAppId": "d1134921-c416-419e-a0a7-0c98e2640e2a"
    },
    "owner": "muje-org",
    "updates": {
      "url": "https://u.expo.dev/75bb3ac1-a958-44c2-989e-6084b19f60ee"
    },
    "runtimeVersion": "1.0.0"
  }
}
